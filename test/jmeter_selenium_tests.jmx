        <com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler guiclass="com.googlecode.jmeter.plugins.webdriver.sampler.gui.WebDriverSamplerGui" testclass="com.googlecode.jmeter.plugins.webdriver.sampler.WebDriverSampler" testname="Login" enabled="true">
          <stringProp name="WebDriverSampler.script">var pkg = JavaImporter(org.openqa.selenium)
var support_ui = JavaImporter(org.openqa.selenium.support.ui.WebDriverWait)
var conditions = org.openqa.selenium.support.ui.ExpectedConditions
var wait = new support_ui.WebDriverWait(WDS.browser, 5000)
//var vars = org.apache.jmeter.threads.JMeterContextService.getContext().getVariables()
//var actions = JavaImporter(org.openqa.selenium.interactions.Actions)
//var builder = new actions.Actions(WDS.browser)
//var thread = java.lang.Thread;
//var keys = org.openqa.selenium.Keys


	WDS.browser.get(&quot;${ENV}&quot;);
	wait.until(conditions.visibilityOfElementLocated(pkg.By.cssSelector(&quot;[data-test=&apos;username&apos;]&quot;))).sendKeys(&quot;standard_user&quot;);
	wait.until(conditions.visibilityOfElementLocated(pkg.By.cssSelector(&quot;[data-test=&apos;password&apos;]&quot;))).sendKeys(&quot;secret_sauce&quot;);
	wait.until(conditions.elementToBeClickable(pkg.By.cssSelector(&quot;[data-test=&apos;login-button&apos;]&quot;))).click();
	
	WDS.sampleResult.sampleStart();
	wait.until(conditions.visibilityOfElementLocated(pkg.By.xpath(&quot;//span[contains(text(),&apos;Products&apos;)]&quot;)));
	WDS.sampleResult.sampleEnd();
</stringProp>
          <stringProp name="WebDriverSampler.parameters"></stringProp>
